    }
  },
  {
    "description": "模拟一个带cookies的post请求:HTTP请求-postwithcookies",
    "request": {
      "uri": "/post/with/cookies",
      "method": "post",
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"lily",
        "age":"18"
      }
    },
    "response": {
      "status":200,
      "json":{
        "lily":"success",
        "status":"1"
      }

